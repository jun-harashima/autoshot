{
  "manifest_version": 3,
  "name": "autoshot",
  "version": "1.0",
  "permissions": [
    "downloads",
    "tabs",
    "scripting",
    "activeTab",
    "debugger"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
